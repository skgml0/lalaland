from itertools import combinations
t=int(input())
for _ in range(t):
    n,m = map(int,input().split())
    # mCn
    result=1
    bunza=1
    bunmo=1
    if n==1:
        result=m
        print(result)
    else:
        for i in range(m,m-n+1,-1):
            #print(" n:{} m:{} i:{} bunza:{}".format(n,m,i,bunza) )
            bunza *=i
            #print(i, bunza )
        for j in range(1,n+1):
            bunmo*=j
        print(bunza//bunmo)
       # for j in range(1,n+1)
